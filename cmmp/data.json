{
	"aprox" : {
		"ec1" : {
			"expr" : "uMax * S / (Ks + Kd + S)",
			"params" : ["uMax", "Ks", "Kd"],
			"in" : ["S"],
			"out" : "u(S)",
			"color" : {"r" : 1.0, "g" : 0.0, "b" : 0.0},
			"iter" : 10000
		},
		"ec2" : {
			"expr" : "uMax * (1 - exp(-S / Ks))",
			"params" : ["uMax", "Ks"],
			"in" : ["S"],
			"out" : "u(S)",
			"color" : {"r" : 1.0, "g" : 1.0, "b" : 0.0},
			"iter" : 10000
		},
		"ec3" : {
			"expr" : "uMax * S / (Ks + S) - Kis * (S - Sc)",
			"params" : ["uMax", "Ks", "Kis", "Sc"],
			"in" : ["S"],
			"out" : "u(S)",
			"color" : {"r" : 1.0, "g" : 0.0, "b" : 1.0},
			"iter" : 10000
		},
		"ec4" : {
			"expr" : "uMax * S^n / (Ks + S^n)",
			"params" : ["uMax", "Ks", "n"],
			"in" : ["S"],
			"out" : "u(S)",
			"color" : {"r" : 0.0, "g" : 1.0, "b" : 1.0},
			"iter" : 10000
		},
		"ec5" : {
			"expr" : "a * x + b",
			"params" : ["a", "b"],
			"in" : ["x"],
			"out" : "f(x)",
			"color" : {"r" : 0.0, "g" : 1.0, "b" : 0.0},
			"iter" : 10000
		}
	},
	"data_sets" : {
		"set1" : "dataset_1.data",
		"set2" : "dataset_2.data"
	},
	"windows" : [
